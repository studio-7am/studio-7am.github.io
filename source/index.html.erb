<div class="container">
  <div class="row">
		<div class="col-lg-12 col-md-12 col-sm-12 col-xs-10">
		  <% page_articles.each_with_index do |article, i| %>
			  <h3><%= link_to article.title, article %> 
			  <small><%= article.date.strftime('%e %b') %></small></h3>
			  <%= article.summary(140, '...') %>
			  <%= link_to 'Читать', article, class: 'btn btn-primary btn-xs' %>
			  <hr>
		  <% end %>
		</div>
   </div>
</div>


